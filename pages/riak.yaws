<html>

<h1>Musiklistan - murvelsork</h1>

<erl>

-define(b2l, binary_to_list).

out(Arg) ->
    Key  = <<"Hello">>,
    Key2 = <<"Bye">>,
    musiklistan:put_kv(Key, <<"The content">>),
    musiklistan:put_kv(Key2, <<"Second content">>),
    Value   = musiklistan:get_v(Key),
    Value2  = musiklistan:get_v(Key2),
    Content = "Key: "    ++ ?b2l(Key)    ++ "<br/>" ++
              "Value: "  ++ ?b2l(Value)  ++ "<br/>" ++
              "</br>"    ++
              "Key2: "   ++ ?b2l(Key2)   ++ "<br/>" ++
              "Value2: " ++ ?b2l(Value2) ++ "<br/>",
    {html, Content}.

</erl>

</html>
