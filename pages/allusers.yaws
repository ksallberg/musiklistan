<head>
    <link href="hacker.css" rel="stylesheet">
</head>

<erl>

out(A) ->
    {atomic, Users} = musiklistan:get_users(),
    FLists = lists:foldl(fun(Name, Acc) ->
                             Acc ++ Name ++ "</br>"
                         end,
                         "",
                         Users),
    {html, "<h1>All users: </h1>" ++ FLists}.
</erl>
