<html>

<h1>Musiklistan - murvelsork</h1>

<erl>

-define(b2l, binary_to_list).
-define(l2b, list_to_binary).

int_to_str(Int) ->
    lists:flatten(io_lib:format("~p", [Int])).

out(Arg) ->
    Ls = [{?l2b("haj"++int_to_str(X)),
           ?l2b("haj"++int_to_str(X))} || X <- lists:seq(0,100)],
    lists:foreach(fun({K, V}) ->
                      musiklistan:put_kv(K, V)
                  end, Ls),
    Content = lists:foldr(fun({K, _}, Acc) ->
                              ?b2l(musiklistan:get_v(K)) ++ "<br/>" ++ Acc
                          end,
                          "",
                          Ls),
    {html, Content}.

</erl>

</html>
